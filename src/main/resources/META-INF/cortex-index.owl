        Created for SOFTWARE purposes                              -->
   <!-- ========================================================== -->
   <!-- Sentinel-3 OLCI/SLSTR/SRAL/Synergie Product                -->
   <!-- ========================================================== -->

   <owl:Class rdf:about="&s3;productMwrOlciSlstrSralSynergie">
      <rdfs:label xml:lang="en">Sentinel-3 OLCI/SLSTR/SRAL/Synergie Product</rdfs:label>
      <rdfs:comment xml:lang="en">
      </rdfs:comment>
      <rdfs:subClassOf rdf:resource="&s3;product"/>
      <drb:signature rdf:parseType="Resource">
         <!--
         <drb:nameMatch>(?i)S3._(OL|SL|SR|SY)_.........[0-9]{8}T[0-9]{6}_[0-9]{8}T[0-9]{6}_[0-9]{8}T[0-9]{6}_[0-9]{4}_[0-9]{3}_[0-9]{3}_.*\.SEN3</drb:nameMatch>
         -->
         <drb:nameMatch>(?i)S3._(MW|OL|SL|SR|SY)_.........[0-9]{8}T[0-9]{6}_[0-9]{8}T[0-9]{6}_[0-9]{8}T[0-9]{6}(?!___________________).*\.SEN3</drb:nameMatch>
      </drb:signature>
   </owl:Class>

